{
  "name": "shiraha-ve",
  "type": "module",
  "version": "0.0.6",
  "private": true,
  "description": "Material 3-inspired Classless CSS Framework.",
  "author": "Ëóç+85CD",
  "license": "WTFPL",
  "homepage": "https://shiraha.js.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/importantimport/shiraha.git",
    "directory": "packages/shiraha"
  },
  "bugs": "https://github.com/importantimport/shiraha/issues",
  "keywords": [
    "classless",
    "css",
    "theme",
    "web-design",
    "reset",
    "style"
  ],
  "exports": {
    ".": "./dist/shiraha.css",
    "./*.css": "./dist/*.css",
    "./*.js": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    },
    "./dist/*.js": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    },
    "./dist/*.css": "./dist/*.css",
    "./src/*": "./src/*"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "devDependencies": {
    "open-props": "^1.6.11"
  }
}